{
  "name": "@portal-payments/solana-wallet-names",
  "version": "1.4.2",
  "description": "Resolves Solana wallet names (.abc, .backpack, .bonk, .glow, .sol, .ottr, .poor and @twitter) to wallet addresses and back.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "test": "jest",
    "clean": "rm -f dist/*",
    "build": "npm run clean && tsup --format cjs,esm",
    "publish-new-version": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/portalpayments/solana-wallet-names.git"
  },
  "keywords": [
    "solana",
    "web3",
    "wallet name",
    "resolve",
    "profile picture",
    "PFP",
    "name service",
    "sns",
    "ans",
    "username"
  ],
  "author": "Mike MacCana <mike@getportal.app>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "jsdom": "^21.1.1"
  },
  "dependencies": {
    "@bonfida/spl-name-service": "^0.1.66",
    "@onsol/tldparser": "^0.5.3",
    "@solana/web3.js": "^1.73.3",
    "@solflare-wallet/pfp": "^0.1.0",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.0.3",
    "fetch-unfucked": "^1.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "vite-plugin-dts": "^2.0.2"
  }
}
